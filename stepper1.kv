<MainScreen>

    name: 'main'
    FloatLayout:
        size_hint: None, None

    DPEAButton:
        id: start_stepper_button
        text: 'motor'
        size: 100, 100
        x: root.width * 0.75
        y: root.height * 0.5
        color: 0, 1, 0, 1
        on_press: root.start_stepper()
        on_release: root.stop_stepper()



